/*******************************************
 **                General                **
 *******************************************/
"#background": {
	bottom: 0,
	left: 0,
	width: '80%',
	image: "/images/background.png",
},

"#scrollContainer": {
	width: Ti.UI.FILL,
	height: Ti.UI.FILL,
	scrollType: "vertical",
	top: 0,
},
"#contentView": {
	width: Ti.UI.FILL,
	height: Ti.UI.SIZE,
	layout: 'vertical',
	top: 0,
},
".whiteContainer": {
	opacity: 1,
	backgroundColor: "white",
	layout: "vertical",
	left: '10%',
	right: '10%',
	top: '1%',
	borderRadius: 5,
	height: Ti.UI.SIZE,
},
".infoGroup": {
	height: Titanium.UI.SIZE,
	width: Titanium.UI.FILL,
},
".static": {
	height: Titanium.UI.FILL,
	width: Titanium.UI.FILL,
},

/*******************************************
 **            Modal Background           **
 *******************************************/
"#opacityBackground":{
	width: Ti.UI.FILL,
	height: Ti.UI.FILL,
	backgroundColor: "#000",
	opacity: 0.3,
},

/*******************************************
 **                TabGroup               **
 *******************************************/
".tabGroup": {
	width: Ti.UI.FILL,
	height: 40,
},
".tabItem": {
	width: '50%',
	height: Ti.UI.FILL,
},
"#leftTabItem": {
	left:0
},
"#rightTabItem": {
	right:0
},
".unselectedTabItem": {
	backgroundGradient : {
		type: "linear",
		startPoint: {x: 0, y: 0},
		endPoint: {x: 0, y: 40},
		colors: [
			{ color: '#bde3dc', offset: 0.0 },
			{ color: '#bde3dc', offset: 0.8 }, 
			{ color: '#737779', offset: 1.0 }
		]
	}
},
".selectedTabItem":{
	backgroundColor: "#e6f6f3"
},
"#tabViewContainer": {
	width: Alloy.Globals.platformWidth,
	height: Titanium.UI.FILL,
},
".tabView": {
	height: Titanium.UI.FILL,
	backgroundColor: "#e6f6f3",
},
"#rightTabView": {
	visible : false,
},
/*******************************************
 **          Fonts & Labels               **
 *******************************************/
"Label":{
	font: {
		fontSize: 17,
	}
},
".boldFont":{
	font: {
		fontSize: 18,
		fontWeight: "bold"
	}
},
".grayFont": {
	color: "#5D5D5D"
},
".buttonLabel": {
	color: '#fff',
	font:{
		fontSize: 22,
		fontWeight: "bold"
	},
	textAlign: Titanium.UI.TEXT_ALIGNMENT_CENTER,
	touchEnabled: false
},
".selectedTabLabel": {
	color: "#00a88a",
	font: {
		fontSize:17,
		fontWeight: "bold"
	}
},
".unselectedTabLabel": {
	color: "#707070",
	font: {
		fontSize:17,
		fontWeight: "bold"
	}
},
".explanationLabel": {
	textAlign: Titanium.UI.TEXT_ALIGNMENT_CENTER,
	backgroundColor: "#E6E6E6",
	height: Ti.UI.SIZE,
	width: Ti.UI.FILL,
	font: {
		fontSize: 14,
	},
},
".fieldLabel": {
	height: 35,
	right: 55,
	left: 20,
	backgroundColor: "#b8e7df",
	color: "gray", 
	textAlign: "center"
},
".infoLabelBold": {
	left: 20,
	font: {
		fontSize: 18,
	},
},
/*******************************************
 **                Shadows                **
 *******************************************/
".fallingShadow":{
	height: 5,
	width: Titanium.UI.FILL,
	backgroundGradient : {
		type: "linear",
		startPoint: {x: 0, y: 0},
		endPoint: {x: 0, y: 5},
		colors: [
			{ color: '#737779', offset: 0.0}, 
			{ color: 'transparent', offset: 1 }
		]
	}
},
".risingShadow":{
	height: 3,
	width: Titanium.UI.FILL,
	backgroundGradient : {
		type: "linear",
		startPoint: {x: 0, y: 3},
		endPoint: {x: 0, y: 0},
		colors: [
			{ color: '#dcdddd', offset: 0.0}, 
			{ color: 'transparent', offset: 1 }
		]
	}
},
/*******************************************
 **           Buttons & Switches          **
 *******************************************/
"Button":{
	backgroundColor: 'transparent'
},
".blueButton": {
	height: 40,
	width: 140,
	borderRadius: 5,
	borderColor:'#102185',
	backgroundGradient: {
		type: "linear",
		startPoint: {
			x: "0",
			y: "0"
	},
	endPoint: {
		x: "0",
		y: "40"
	},
	colors: [{ color: '#404D9D', offset: 0.5 }, { color: '#102185', offset: 0.5 } ]
	}
},
".aquaButton": {
	height: 40,
	width: 140,
	borderRadius: 5,
	borderColor:'#007C5C',
	backgroundGradient: {
		type: "linear",
		startPoint: {
			x: "0",
			y: "0"
	},
	endPoint: {
		x: "0",
		y: "40"
	},
	colors: [{ color: '#33967D', offset: 0.5 }, { color: '#007C5C', offset: 0.5 } ]
	}
},
".shinnyGrayButton": {
	height: 35,
	width: 35,
	right: 20,
	borderColor: "#cfcfcf",
	backgroundGradient : {
		type: "linear",
		startPoint: {x: 20, y: 0},
		endPoint: {x: 20, y: 35},
		colors: [
			{ color: '#dfdfdf', offset: 0.0}, 
			{ color: 'white', offset: 0.5 },
			{ color: '#dfdfdf', offset: 1 }
		]
	},
},
/*******************************************
 **                 Lines                 **
 *******************************************/
".blueGradientLine": {
	height:"1",
	left: 10,
	right: 10,
	backgroundGradient: {
		type: "linear",
		startPoint: {
			x: "0",
			y: "0"
	},
	endPoint: {
		x: Alloy.Globals.platformWidth,
		y: "0"
	},
	colors: [ { color: 'transparent', offset: 0.0}, { color: '#00007F', offset: 0.5 }, { color: 'transparent', offset: 1.0 } ]
	}
},
".greenLine": {
	height:"1",
	backgroundColor: "#9ac6bd",
},
".greenGradientLine": {
	height: 1,
	width: Titanium.UI.FILL,
	backgroundGradient: {
		type: "linear",
		startPoint: {
			x: "0",
			y: "0"
	},
	endPoint: {
		x: Alloy.Globals.platformWidth,
		y: "0"
	},
	colors: [ { color: 'transparent', offset: 0.0}, { color: '#00a88a', offset: 0.5 }, { color: 'transparent', offset: 1.0 } ]
	}
},
".blueLine": {
	height:"1",
	backgroundColor: "#00007F"
},
/*******************************************
 **                Tables                 **
 *******************************************/
".darkRow": {
	className: 'contactRow',
	backgroundColor: "#d6ede9",
	selectionStyle: "NONE",
	touchEnabled: true,
	//hasChild: true,
	height: 35,
	opacity: 0.7
},
".lightRow": {
	className: 'contactRow',
	backgroundColor: "#eaf6f4",
	selectionStyle: "NONE",
	touchEnabled: true,
	//hasChild: true,
	height: 35,
	opacity: 0.7
},
