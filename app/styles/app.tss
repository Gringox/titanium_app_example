/*******************************************
 **                  iOS                  **
 *******************************************/
"Window[platform=ios]":{
	orientationModes: [Titanium.UI.PORTRAIT, Titanium.UI.UPSIDE_PORTRAIT],
	navBarHidden: true,
	fullscreen: false,
	backgroundColor: "#fff",
	//image: "/images/background.png",
},
"#container[platform=ios]": {
	width: Alloy.Globals.platformWidth,
	top: 60,
	bottom: 45,
	fontFamily: 'Helvetica Neue',
	font : {
		fontSize: 18
	},
},
".logoImg[platform=ios]": {
	top: "5%",
	left: "10%",
	right: "10%"
},
".scrollContainerGradient": {
	backgroundGradient: {
		type: "linear",
		startPoint: {
			x: "0",
			y: "0"
		},
		endPoint: {
			x: 0,
			y: Alloy.Globals.platformHeight
		},
		colors: [ { color: 'white', offset: 0}, { color: '#A1C4BD', offset: 0.4 }, { color: '#A1C4BD', offset: 0.6 }, { color: 'white', offset: 0.8 } ],
	},
	opacity: 0.7,
	height: Titanium.UI.FILL
},
".transactionContainerGradient": {
	backgroundGradient: {
		type: "linear",
		startPoint: {
			x: "0",
			y: "0"
		},
		endPoint: {
			x: 0,
			y: Alloy.Globals.platformHeight
		},
		colors: [ { color: 'white', offset: 0}, { color: '#A1C4BD', offset: 0.2 }, { color: '#A1C4BD', offset: 0.4 }, { color: 'white', offset: 0.6 } ],
	},
	opacity: 0.7,
	height: Titanium.UI.FILL
},
".signUpContainerGradient": {
	backgroundGradient: {
		type: "linear",
		startPoint: {
			x: "0",
			y: "0"
		},
		endPoint: {
			x: 0,
			y: Alloy.Globals.platformHeight
		},
		colors: [ { color: 'white', offset: 0.2}, { color: '#A1C4BD', offset: 0.4 }, { color: '#A1C4BD', offset: 0.6 }, { color: 'white', offset: 0.8 } ],
	},
	opacity: 0.7,
	height: Titanium.UI.FILL
},
"#searchBar[platform=ios]": {
	width: Ti.UI.FILL,
	height: 60,
	barColor: "#e6f6f3",
}
/*******************************************
 **                Android                **
 *******************************************/
